{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "max-len": ["error", { "ignoreComments": true }],
    "class-methods-use-this": "off",
    "arrow-parens": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "object-curly-newline": "off",
    "require-jsdoc": "off",
    "indent": ["error", 2]
  }
}
